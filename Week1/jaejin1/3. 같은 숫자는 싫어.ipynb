    "def no_continuous(s):\n",
    "    if not s:\n",
    "        return []\n",
    "    temp = s[0]\n",
    "    result = [temp]\n",
    "    for i in s:\n",
    "        if i != temp:\n",
    "            result.append(i)\n",
    "            temp = i\n",
    "    return result\n",
    "    \n",
    "no_continuous('133303')"
   ]
