    "def palindrome(s):\n",
    "    if s==s[::-1]:\n",
    "        return len(s)\n",
    "    return max(palindrome(s[:-1]),palindrome(s[1:]))\n",
    "\n",
    "print(palindrome(\"abcdcba\"))\n",
    "print(palindrome(\"abacde\"))"
   ]
  }
